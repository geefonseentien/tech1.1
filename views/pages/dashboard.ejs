<!DOCTYPE html>
<html lang="nl">
<head>
    <%- include('../partials/head'); %>
</head>
<body>
    <header>
        <%- include('../partials/logged-in'); %>
    </header>
    <main class="background_img">
        <div class="overlay"></div>
        <section class="active_users">
            <h3>Vind nu je Apres Ski maatje</h3>
            <div>
                <article>
                    <% users.forEach(function(users) { %>
                        <li>
                            <img src="../../img/no-user.png" alt="no user image">
                            <p><%= users.name %></p>
                            <%- include('../partials/like-form'); %>
                        </li>
                    <% }) %>
                </article>
            </div>
        </section>
    </main>
    <footer>
        <%- include('../partials/footer'); %>
    </footer>
</body>
</html>